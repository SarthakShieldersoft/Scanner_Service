{
  "name": "code-scanner-service",
  "version": "1.0.0",
  "description": "A service to analyze code for vulnerabilities using Gemini API.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node test-scanner.js",
    "test:health": "node test-scanner.js --health-only",
    "test:repo": "node test-scanner.js --repo-only",
    "test:legacy": "node test-scanner.js --legacy-only",
    "test:comprehensive": "node comprehensive-test.js",
    "test:full": "node comprehensive-test.js",
    "test:quick": "node comprehensive-test.js --health --legacy --database",
    "test:integration": "node comprehensive-test.js --integration",
    "test:performance": "node comprehensive-test.js --performance"
  },
